{
	"name": "better-zastupovani",
	"version": "1.0.0",
	"description": "Better Zastupovani",
	"main": "dist/index.html",
	"private": true,
	"scripts": {
		"build": "webpack --config webpack.prod.js",
		"dev": "webpack-dev-server --open --config webpack.dev.js",
		"test": "mocha ./src/**/*.test.ts",
		"prepush": "npm run lint && npm run test",
		"lint": "tslint src/**/*.ts --project tsconfig.json --fix",
		"ngrok": "ngrok http 192.168.1.200:8080 -host-header=\"192.168.1.200:8080\" -region eu",
		"test-server-dev": "webpack-dev-server --config webpack.dev.js",
		"test-server-prod": "http-server ./dist",
		"cy:run": "cypress run",
		"cy:run:record": "cypress run --record --key 11118fd8-27c7-4ee0-8c61-4ab69625c7b6",
		"cy:open": "cypress open",
		"cy:local:dev:run": "start-server-and-test test-server-dev http://192.168.1.200:8080 cy:run",
		"cy:local:dev:open": "start-server-and-test test-server-dev http://192.168.1.200:8080 cy:open",
		"cy:local:prod:open": "npm run build && start-server-and-test test-server-prod http://192.168.1.200:8080 cy:open",
		"cy:local:prod:run": "npm run build && start-server-and-test test-server-prod http://192.168.1.200:8080 cy:run",
		"cy:ci:prod:run": "start-server-and-test test-server-prod http://127.0.0.1:8080 cy:run:record"
	},
	"dependencies": {
		"array2d": "0.0.5",
		"date-fns": "^1.29.0",
		"driver.js": "^0.2.4",
		"hammerjs": "^2.0.8",
		"jquery": "^3.3.1",
		"js-cookie": "^2.2.0",
		"ms": "^2.1.1",
		"raven-js": "^3.22.4",
		"whatwg-fetch": "^2.0.3"
	},
	"devDependencies": {
		"@cypress/webpack-preprocessor": "^1.1.3",
		"@sentry/webpack-plugin": "^1.4.0",
		"@types/chai": "^4.1.2",
		"@types/jquery": "^3.3.0",
		"@types/mocha": "^2.2.48",
		"@types/node": "^8.9.4",
		"@types/puppeteer": "^1.0.0",
		"babel-polyfill": "^6.26.0",
		"babel-preset-env": "^1.6.1",
		"babel-preset-es2015": "^6.24.1",
		"chai": "^4.1.2",
		"clean-webpack-plugin": "^0.1.18",
		"css-loader": "^0.28.10",
		"cypress": "^2.1.0",
		"eslint": "^4.18.2",
		"eslint-config-google": "^0.9.1",
		"eslint-plugin-cypress": "^2.0.1",
		"extract-text-webpack-plugin": "^3.0.2",
		"file-loader": "^0.11.2",
		"global-jsdom": "^4.2.0",
		"http-server": "^0.11.1",
		"husky": "^0.14.3",
		"iconv-lite": "^0.4.19",
		"js-sha256": "^0.9.0",
		"jsdom": "11.6.2",
		"mocha": "^5.0.4",
		"ngrok": "^3.0.0",
		"request": "^2.83.0",
		"start-server-and-test": "^1.1.0",
		"style-loader": "^0.18.2",
		"ts-loader": "^3.5.0",
		"ts-node": "^4.1.0",
		"tslint": "^5.9.1",
		"typescript": "^2.7.2",
		"webpack": "^3.11.0",
		"webpack-closure-compiler": "^2.1.6",
		"webpack-dev-server": "^2.11.2",
		"webpack-merge": "^4.1.2"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/JouzaLoL/better-zastupovani.git"
	},
	"author": "Josef Vacek",
	"license": "CC BY-NC-ND 4.0",
	"bugs": {
		"url": "https://github.com/JouzaLoL/better-zastupovani/issues"
	},
	"homepage": "https://github.com/JouzaLoL/better-zastupovani#readme"
}
