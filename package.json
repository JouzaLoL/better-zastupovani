{
	"name": "better-zastupovani",
	"version": "1.0.0",
	"description": "Better Zastupovani",
	"main": "dist/index.html",
	"private": true,
	"scripts": {
		"build": "webpack --config webpack.prod.js",
		"dev": "webpack-dev-server --config webpack.dev.js",
		"test": "jest",
		"prepush": "npm run lint",
		"lint": "tslint src/**/*.ts --project tsconfig.json --fix",
		"test-server-prod": "http-server ./dist --silent",
		"cy:run": "cypress run",
		"cy:run:record": "cypress run --record",
		"cy:open": "cypress open",
		"cy:local:prod:run": "npm run build && start-server-and-test test-server-prod http://192.168.88.200:8080 cy:run",
		"cy:ci:prod:run": "start-server-and-test test-server-prod http://127.0.0.1:8080 cy:run:record",
		"test-deploy": "gh-pages -d dist -r git@github.com:JouzaLoL/test-site.git -b master"
	},
	"dependencies": {
		"array2d": "0.0.5",
		"date-fns": "^2.0.1",
		"driver.js": "^0.9.7",
		"hammerjs": "^2.0.8",
		"is-mobile": "^0.2.2",
		"jquery": "^3.4.1",
		"js-cookie": "^2.2.1",
		"raven-js": "^3.27.2",
		"whatwg-fetch": "^2.0.3"
	},
	"devDependencies": {
		"@cypress/webpack-preprocessor": "^1.1.3",
		"@types/cypress": "^1.1.3",
		"@types/jest": "^24.0.18",
		"babel-polyfill": "^6.26.0",
		"babel-preset-es2015": "^6.24.1",
		"clean-webpack-plugin": "^3.0.0",
		"css-loader": "^3.2.0",
		"cypress": "^3.4.1",
		"cypress-eslint-preprocessor": "^1.2.2",
		"eslint": "^6.3.0",
		"eslint-config-google": "^0.13.0",
		"eslint-plugin-cypress": "^2.6.1",
		"file-loader": "^4.2.0",
		"gh-pages": "^2.1.1",
		"global-jsdom": "^5.0.0",
		"http-server": "^0.11.1",
		"jest": "^24.9.0",
		"js-sha256": "^0.9.0",
		"mini-css-extract-plugin": "^0.8.0",
		"request": "^2.88.0",
		"start-server-and-test": "^1.10.0",
		"style-loader": "^1.0.0",
		"ts-jest": "^24.0.2",
		"ts-loader": "^6.0.4",
		"ts-node": "^8.3.0",
		"tsconfig-paths": "^3.8.0",
		"tslint": "^5.19.0",
		"typescript": "^3.6.2",
		"uglifyjs-webpack-plugin": "^2.2.0",
		"webpack": "^4.39.3",
		"webpack-cli": "^3.3.7",
		"webpack-dev-server": "^3.8.0",
		"webpack-merge": "^4.2.2"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/JouzaLoL/better-zastupovani.git"
