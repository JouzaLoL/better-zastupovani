{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "node",
		"baseUrl": ".",
		"outDir": "./dist/",
		"sourceMap": true,
		"noImplicitAny": false,
		"target": "esnext",
		"allowJs": true,
		"lib": [
			"dom",
			"webworker",
			"esnext",
			"dom.iterable",
			"es7"
		]
	},
	"typeAcquisition": {
		"enable": true
	}
}